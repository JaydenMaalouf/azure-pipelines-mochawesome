{
  "manifestVersion": 1,
  "id": "allure-reporter",
  "publisher": "JaydenMaalouf",
  "version": "1.0.6",
  "name": "Allure Reporter",
  "description": "Azure DevOps Extension",
  "categories": ["Azure Pipelines"],
  "targets": [
  {
    "id": "Microsoft.VisualStudio.Services"
  }
  ],
  "files": [
  {
    "path": "tasks/UploadAllureReport"
  }
  ],
  "contributions": [
  {
    "id": "pipeline-allure-tab",
    "type": "ms.vss-build-web.build-results-tab",
    "description": "Embed Allure HTML report in Azure Pipelines Build",
    "targets": [
    "ms.vss-build-web.build-results-view"
    ],
    "properties": {
    "name": "Allure",
    "uri": "dist/tabContent.html",
    "registeredObjectId": "registerBuild",
    "supportsTasks": ["2f1fb45f-1828-460a-8f2e-ec5c31c36241"],
    "dynamic": true
    }
  },
  {
    "id": "UploadAllureReport",
    "type": "ms.vss-distributed-task.task",
    "targets": ["ms.vss-distributed-task.tasks"],
    "properties": {
    "name": "tasks/UploadAllureReport"
    }
  }
  ]
}
